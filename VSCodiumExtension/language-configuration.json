{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["(", ")"],
    ["<", ">"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "<", "close": ">" }
  ],
  "folding": {
    "markers": {
      "start": "^(#region|#[-]{3,})",
      "end": "^(#endregion|#[-]{3,})"
    }
  }
}
