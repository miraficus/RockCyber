{
  "name": "rockbox-wps-sbs-syntax",
  "displayName": "Rockbox WPS/SBS Syntax",
  "description": "Syntax highlighting + autocomplete + hover help + folding for Rockbox .wps and .sbs skin files",
  "version": "1.0.0",
  "publisher": "MiraFicus",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "rockbox-wps",
        "aliases": ["Rockbox WPS/SBS", "rockbox-wps"],
        "extensions": [".wps", ".sbs"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rockbox-wps",
        "scopeName": "source.rockbox-wps",
        "path": "./syntaxes/rockbox-wps.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "rockbox-wps",
        "path": "./snippets/rockbox-wps.json"
      }
    ]
  }
}
